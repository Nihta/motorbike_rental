<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <!-- Menu root -->
  <menuitem name="Motorbike rental" id="mbr_menu_root"
            web_icon="motorbike_rental,static/description/icon.png"/>

  <!-- Submenu -->
  <menuitem name="Motorcycle" id="mbr_motorcycle_menu" parent="mbr_menu_root"
            action="mbr_motorcycle_action" sequence="10"/>
  <menuitem name="Customers" id="mbr_customer_menu" parent="mbr_menu_root"
            action="mbr_customer_action" sequence="20"/>
  <menuitem name="Rentals" id="mbr_rental_menu" parent="mbr_menu_root"
            action="mbr_rental_action" sequence="30"/>
  <menuitem name="Configs" id="mbr_motorcycle_configs_menu" parent="mbr_menu_root" sequence="90"/>


  <!-- Actions -->
  <menuitem name="Motorcycle" id="mbr_menu_motorcycle" parent="mbr_motorcycle_configs_menu">
    <menuitem name="Motorcycle model" id="mbr_motorcycle_model_menu_action"
              action="mbr_motorcycle_model_action"/>
    <menuitem name="Motorbike category" id="mbr_motorcycle_category_menu_action"
              action="mbr_motorcycle_category_action"/>
    <menuitem name="Motorbike brand" id="mbr_motorcycle_brand_menu_action"
              action="mbr_motorcycle_brand_action"/>
  </menuitem>

  <menuitem id="mbr_menu_localisation" name="Localization" parent="mbr_motorcycle_configs_menu">
    <menuitem name="Country" id="mbr_motorcycle_country_menu_action" sequence="1"
              action="base.action_country"/>
    <menuitem name="State" id="mbr_motorcycle_country_state_menu_action" sequence="2"
              action="base.action_country_state"/>
  </menuitem>

</odoo>

