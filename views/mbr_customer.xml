<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="mbr_customer_action" model="ir.actions.act_window">
    <field name="name">Customer</field>
    <field name="res_model">mbr.customer</field>
    <field name="view_mode">tree,form</field>
    <field name="help" type="html">
      <p class="o_view_nocontent_smiling_face">
        No motorcycle brand yet.
      </p>
    </field>
  </record>


  <record id="mbr_customer_view_tree" model="ir.ui.view">
    <field name="name">mbr.customer.tree</field>
    <field name="model">mbr.customer</field>
    <field name="arch" type="xml">
      <tree string="Motorcycle">
        <field name="full_name"/>
        <field name="identification_id"/>
        <field name="phone"/>
        <field name="email"/>
      </tree>
    </field>
  </record>

</odoo>
